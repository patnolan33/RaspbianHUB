{
  "name": "raspbian-hub",
  "productName": "RaspbianHUB",
  "version": "1.0.0",
  "description": "Home system for Raspbian touchscreen",
  "main": "main.js",
  "config": {
    "port": "8080"
  },
  "scripts": {
    "version": "electron -v",
    "start": "cross-env NODE_ENV=development PORT=$npm_package_config_port electron .",
    "build": "./node_modules/.bin/webpack",
    "watch": "cross-env PORT=$npm_package_config_port ./node_modules/.bin/webpack-dev-server --hot --inline",
    "build:prod": "cross-env PORT=$npm_package_config_port webpack --env.prod -p",
    "prepare": "node ./node_modules/patch-package/index.js",
    "launch": "cross-env PORT=$npm_package_config_port parallel \"watch --raspbian-hub:port=$npm_package_config_port\" \"start --raspbian-hub:port=$npm_package_config_port\"",
    "build:launch": "cross-env PORT=$npm_package_config_port parallel \"build:prod --raspbian-hub:port=$npm_package_config_port\" \"launch --raspbian-hub:port=$npm_package_config_port\""
  },
  "author": "Patrick Nolan",
  "license": "MIT",
  "dependencies": {
    "@turf/turf": "4.7.3",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babelify": "8.0.0",
    "css-loader": "0.28.5",
    "deepcopy": "0.6.3",
    "electron-prebuilt": "1.4.13",
    "electron-reload": "1.2.2",
    "es6-map": "^0.1.5",
    "material-ui": "0.20.0",
    "moment": "^2.20.1",
    "net": "^1.0.2",
    "python-shell": "^0.4.0",
    "react": "15.4.2",
    "react-bootstrap": "^0.32.0",
    "react-color": "^2.13.8",
    "react-dom": "15.4.2",
    "request": "^2.83.0",
    "style-loader": "0.18.2",
    "tls": "0.0.1",
    "ts-loader": "3.2.0",
    "typescript": "2.6.2",
    "url-loader": "0.5.9",
    "webpack": "3.10.0"
  },
  "devDependencies": {
    "@types/material-ui": "0.18.5",
    "@types/moment": "^2.13.0",
    "@types/node": "8.0.24",
    "@types/python-shell": "^0.4.1",
    "@types/react-bootstrap": "^0.32.0",
    "@types/react-color": "^2.13.3",
    "@types/react-dom": "16.0.3",
    "@types/request": "^2.0.10",
    "browserify": "13.1.0",
    "electron-rebuild": "1.6.0",
    "npm-parallel": "^2.0.1",
    "patch-package": "^5.0.0",
    "react-addons-perf": "15.4.2",
    "tsify": "3.0.4",
    "vinyl-buffer": "1.0.0",
    "vinyl-source-stream": "1.1.0",
    "watchify": "3.9.0",
    "webpack-dev-server": "2.9.7"
  }
}
